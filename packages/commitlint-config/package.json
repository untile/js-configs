{
  "name": "@untile/commitlint-config",
  "version": "2.0.0",
  "description": "Untile-flavored commitlint config",
  "keywords": [
    "config",
    "commit",
    "commitlint",
    "lint"
  ],
  "homepage": "https://github.com/untile/js-configs/tree/master/packages/commitlint-config/#readme",
  "bugs": {
    "url": "https://github.com/untile/js-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/untile/js-configs.git"
  },
  "license": "MIT",
  "main": "src",
  "scripts": {
    "lint": "eslint .",
    "release": "../../bin/release.sh @untile/commitlint-config",
    "test": "true",
    "test:watch": "jest --watch --notify"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "resolutions": {
    "decode-uri-component": "^0.2.1",
    "json5": "^2.2.1",
    "moment": "^2.29.4"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^17.6.7",
    "commitlint-plugin-function-rules": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/lint": "^17.6.7",
    "@commitlint/load": "^17.6.7",
    "@untile/eslint-config": "^2.0.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.1"
  },
  "peerDependencies": {
    "@commitlint/cli": "^17.6.7"
  },
  "engines": {
    "node": ">=16"
  }
}
