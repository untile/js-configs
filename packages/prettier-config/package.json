{
  "name": "@untile/prettier-config",
  "version": "1.0.2",
  "description": "Untile-flavored Prettier config",
  "keywords": [
    "config",
    "prettier",
    "lint"
  ],
  "homepage": "https://github.com/untile/js-configs/tree/master/packages/prettier-config/#readme",
  "bugs": {
    "url": "https://github.com/untile/js-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/untile/js-configs.git"
  },
  "license": "MIT",
  "main": "src",
  "scripts": {
    "lint": "eslint . --ignore-pattern 'test/fixtures/*'",
    "release": "../../bin/release.sh @untile/prettier-config",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "lint-staged": {
    "!(test/fixtures/**/*)*.js": [
      "eslint"
    ],
    "package.json": [
      "sort-package-json"
    ],
    "yarn.lock": [
      "yarn-deduplicate"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/parser": "^7.22.7",
    "@untile/eslint-config": "^1.0.3",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "prettier": "3"
  },
  "engines": {
    "node": ">=16"
  }
}
