{
  "name": "@untile/eslint-config-typescript-react-native",
  "version": "2.0.1",
  "description": "Untile-flavored Typescript ESLint react-native config",
  "keywords": [
    "config",
    "eslint",
    "lint",
    "react-native",
    "typescript"
  ],
  "homepage": "https://github.com/untile/js-configs/tree/master/packages/eslint-config-typescript-react-native/#readme",
  "bugs": {
    "url": "https://github.com/untile/js-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/untile/js-configs.git"
  },
  "license": "MIT",
  "main": "src",
  "scripts": {
    "lint": "eslint . --ignore-pattern 'test/fixtures/*' --config src/index.js",
    "release": "../../bin/release.sh @untile/eslint-config-typescript-react-native",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --runInBand",
    "test:watch": "jest --watch --notify"
  },
  "lint-staged": {
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@untile/eslint-config-typescript-react": "^2.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "typescript-eslint": "^8.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@types/jest": "^29.5.14",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.1",
    "jest": "^29.7.0",
    "react": "^19.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "eslint": "9",
    "typescript": ">=4.9.0 || >=5.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
