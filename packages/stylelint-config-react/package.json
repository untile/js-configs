{
  "name": "@untile/stylelint-config-react",
  "version": "1.0.2",
  "description": "Untile-flavored stylelint config react",
  "keywords": [
    "config",
    "react",
    "styled-components",
    "stylelint",
    "untile"
  ],
  "homepage": "https://github.com/untile/js-configs/tree/master/packages/stylelint-config-react#readme",
  "bugs": {
    "url": "https://github.com/untile/js-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/untile/js-configs.git"
  },
  "license": "MIT",
  "main": "src",
  "scripts": {
    "lint": "eslint . --ignore-pattern 'test/fixtures/*'",
    "release": "../../bin/release.sh @untile/stylelint-config-react",
    "test": "jest",
    "test:watch": "jest --watch --notify"
  },
  "lint-staged": {
    "!(test/fixtures/**/*)*.js": [
      "eslint"
    ],
    "package.json": [
      "sort-package-json"
    ],
    "yarn.lock": [
      "yarn-deduplicate"
    ]
  },
  "resolutions": {
    "json5": "^2.2.1",
    "minimatch": "^3.0.5",
    "moment": "^2.29.4"
  },
  "dependencies": {
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@untile/eslint-config": "^1.0.3",
    "babel-jest": "^29.4.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.38.0",
    "jest": "^29.4.1",
    "postcss": "^8.4.21",
    "postcss-styled-syntax": "^0.4.0",
    "stylelint": "^15.4.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.21",
    "postcss-styled-syntax": "^0.4.0",
    "stylelint": ">=15.3"
  },
  "engines": {
    "node": ">=14.18"
  }
}
