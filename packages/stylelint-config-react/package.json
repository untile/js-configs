{
  "name": "@untile/stylelint-config-react",
  "version": "0.0.0",
  "description": "Untile-flavored stylelint config react",
  "keywords": [
    "config",
    "react",
    "styled-components",
    "stylelint",
    "untile"
  ],
  "homepage": "https://github.com/untile/js-configs/packages/stylelint-config-react/#readme",
  "bugs": {
    "url": "https://github.com/untile/js-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/untile/js-configs.git"
  },
  "license": "MIT",
  "main": "src",
  "scripts": {
    "lint": "eslint . --ignore-pattern 'test/fixtures/*'",
    "release": "../../bin/release.sh @untile/stylelint-config-react",
    "test": "jest",
    "test:watch": "jest --watch --notify"
  },
  "lint-staged": {
    "!(test/fixtures/**/*)*.js": [
      "eslint"
    ],
    "package.json": [
      "sort-package-json"
    ],
    "yarn.lock": [
      "yarn-deduplicate"
    ]
  },
  "dependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "postcss": "^8.4.21",
    "postcss-syntax": "^0.36.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@untile/eslint-config": "*",
    "babel-jest": "^29.4.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.35.0",
    "jest": "^29.4.1",
    "stylelint": "^15.2.0"
  },
  "peerDependencies": {
    "stylelint": "15"
  },
  "engines": {
    "node": ">=14.18"
  }
}
