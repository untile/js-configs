{
  "name": "@untile/stylelint-config-react-native",
  "version": "2.0.1",
  "description": "Untile-flavored stylelint config react",
  "keywords": [
    "config",
    "react",
    "styled-components",
    "stylelint",
    "untile"
  ],
  "homepage": "https://github.com/untile/js-configs/tree/master/packages/stylelint-config-react-native/#readme",
  "bugs": {
    "url": "https://github.com/untile/js-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/untile/js-configs.git"
  },
  "license": "MIT",
  "main": "src",
  "scripts": {
    "lint": "eslint . --ignore-pattern 'test/fixtures/*'",
    "release": "../../bin/release.sh @untile/stylelint-config-react-native",
    "test": "jest",
    "test:watch": "jest --watch --notify"
  },
  "lint-staged": {
    "!(test/fixtures/**/*)*.js": [
      "eslint"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@untile/stylelint-config-react": "^2.0.1",
    "stylelint-react-native": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@untile/eslint-config": "^2.0.1",
    "babel-jest": "^29.6.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.1",
    "stylelint": "^15.10.2"
  },
  "engines": {
    "node": ">=16"
  }
}
